{
  "name": "login",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "5.0.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "mongodb": "3.6.3",
    "mongoose": "5.11.8",
    "mongoose-date-format": "1.2.0",
    "path": "0.12.7"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "nodemon": "2.0.6"
